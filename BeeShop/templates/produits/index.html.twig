{% extends 'base.html.twig'%}

{% block title 'Tous nos produits '%}

{% block body %}

<div class = "container">
    <h1> Les biens disponibles </h1>

    <div class="row flex">
    {% for prop in produits %}
        <div class="col-3">
        <div clas="card">
        <div clas="card-body">
        <h5 clas="card-title">
            <a href="#">{{prop.nomProduit}}</a>
        </h5>
        <p class="card-text"> {{prop.description }} </p>
        <div class="text-primary"> {{prop.prix}}</div>
        </div>
        </div>
        </div>
        {% endfor %}
    </div>
</div>

{% endblock %}

