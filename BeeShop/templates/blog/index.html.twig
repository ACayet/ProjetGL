{% extends 'base.html.twig' %}

{% block title %} BeeBlog {% endblock %}

{% block body %}

<h1>Bienvenue sur BeeBlog</h1>

    <p>Ci-dessous vous trouverez les articles : </p>

<div class="example-wrapper">
    <div class="container">
        {% for art in articles %}
              <h3> <a href="{{ path('art.show', {id: art.id, slug: art.slug}) }}"> {{art.titre}} </a> </h3>
                             
        {% endfor %}
    </div>
    
    
</div>
{% endblock %}
